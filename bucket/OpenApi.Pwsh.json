{
    "version": "0.7.0",
    "autoupdate": {
        "hash": {
            "regex": "<d:PackageHash>$base64<.d:PackageHash>",
            "url": "https://www.powershellgallery.com/api/v2/Packages()?$filter=Id eq 'OpenApi.Pwsh' and Version eq '$version'"
        },
        "url": "https://www.powershellgallery.com/api/v2/Package/OpenApi.Pwsh/$version#/mod.nupkg"
    },
    "psmodule": {
        "name": "OpenApi.Pwsh"
    },
    "homepage": "https://www.powershellgallery.com/packages/OpenApi.Pwsh",
    "description": "Import, edit, and invoke OpenAPI documents.",
    "url": "https://www.powershellgallery.com/api/v2/Package/OpenApi.Pwsh/0.7.0#/mod.nupkg",
    "hash": "sha512:f371dd341c0ee015fe4314789c0f6f8013306432165558d41b460dc03405e957a9ac00a725f409154d27d8eabb766c36050dc577aa0cc4361b04b991e6f27047",
    "license": {
        "url": "https://github.com/matt9ucci/OpenApi.Pwsh/blob/master/LICENSE",
        "identifier": "Refer to Webpage"
    },
    "checkver": {
        "url": "https://www.powershellgallery.com/api/v2/Packages()?$filter=Id%20eq%20'OpenApi.Pwsh'%20and%20IsLatestVersion%20eq%20true",
        "regex": "(?i)<d:Version>(?<ver>[^<]+)<.d:Version>",
        "replace": "${ver}"
    }
}
